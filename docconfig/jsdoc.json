{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc"
    ]
  },
  "source": {
    "include": [
      "./dist"
    ]
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "readme": "docs.md",
    "template": "docconfig/template",
    "encoding": "utf8",
    "destination": "./docs",
    "recurse": true
  },
  "templates": {
    "cleverLinks": false
  }
}
